    <div class="container">
    <h1 class="text-primary">Create New Campground</h1>
    <div class="row">
    <div class="col-4">
    <form class="mb-3" action="/campgrounds" method="post" enctype="multipart/form-data">
        <label class="form-label" for="title">Name</label>
        <input class="form-control mb-3" type="text" name="title">
        <label class="form-label" for="location">Location</label>
        <input class="form-control" type="text" name="location">
        <label class="form-label" for="image">Image</label>
        <input class="form-control mb-3" type="file" name="image">
        <label class="form-label" for="price">Price</label>
        <input class="form-control mb-3" type="text" name="price">
        <label class="form-label" for="Description">Description</label>
        <textarea class="form-control mb-3" type="text" name="Description"></textarea>
        <button class="btn btn-primary mt-3" type="submit">Add Campground</button>
    </form>
    <a href="/campgrounds">All Campgrounds</a>
    </div>
    </div>
    </div>